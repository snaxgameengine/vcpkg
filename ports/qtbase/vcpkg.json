    "opengl": {
      "description": "OpenGL",
      "dependencies": [
        "opengl",
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "gui"
          ]
        }
      ]
    },
    "openssl": {
      "description": "Enable OpenSSL",
      "dependencies": [
        "openssl",
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "network"
          ]
        }
      ]
    },
    "pcre2": {
      "description": "Enable PCRE2 support",
      "dependencies": [
        "pcre2"
      ]
    },
    "png": {
      "description": "Enable PNG",
      "dependencies": [
        "libpng",
        {
